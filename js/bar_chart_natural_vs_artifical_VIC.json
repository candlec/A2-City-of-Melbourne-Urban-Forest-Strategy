{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "https://raw.githubusercontent.com/candlec/A2-City-of-Melbourne-Urban-Forest-Strategy/refs/heads/main/data/1988-2024-Victoria-Land-Coverage-modified-no-comma.csv",
    "format": {
      "type": "csv",
      "parse": {"Total": "integer"}
    }
  },
  "transform": [{"filter": {"field": "LandCoverClasses", "oneOf" : ["Artificial surfaces", "Natural surfaces"]}}],
  "title": "Victorian Artifical and Natural Surface Split",
  "width": 400,
  "height": 300,
  "mark": "bar",
  "encoding": {
    "x":{
        "field": "Year",
        "timeUnit": "year",
        "type": "nominal",
        "title": ""
    },
    "color": {
      "field": "LandCoverClasses",
      "scale": {"range": ["#587470", "#A3F5CC", "#BF364D", "#7DB800", "#764134", "#B88F65", "#49AB8C", "#6EC4BD", "#89C9E6", "#74B4BE", "#0081A7", "#C1C3C3"]},
      "title": "Land cover Type",
      "legend": {"direction": "horizontal", "orient": "bottom"}
    },
    "y": {
        "field": "Total",
        "type": "quantitative",
        "sort": "ascending",
        "title": "Total Hectares"
    },
    "tooltip": [{"field": "Total", "title": "Total Hectares"},
      {"field": "Year", "timeUnit": "year", "title": "Year"}]
  }
}